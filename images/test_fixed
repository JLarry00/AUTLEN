digraph {
	rankdir=LR
	"" [shape=none]
	q0 [shape=circle]
	q9 [shape=doublecircle]
	q7 [shape=circle]
	q5 [shape=circle]
	q3 [shape=circle]
	q1 [shape=circle]
	q2 [shape=circle]
	q4 [shape=circle]
	q6 [shape=circle]
	q8 [shape=circle]
	"" -> q0
	q0 -> q1 [label=H]
	q2 -> q3 [label=e]
	q1 -> q2 [label="位"]
	q4 -> q5 [label=l]
	q3 -> q4 [label="位"]
	q6 -> q7 [label=l]
	q5 -> q6 [label="位"]
	q8 -> q9 [label=o]
	q7 -> q8 [label="位"]
}
