// Automata
digraph {
	rankdir=LR
	"" [shape=none]
	"{'q2', 'q4', 'q0'}" [shape=doublecircle]
	"{'q1', 'q3', 'q5'}" [shape=circle]
	"" -> "{'q2', 'q4', 'q0'}"
	"{'q2', 'q4', 'q0'}" -> "{'q1', 'q3', 'q5'}" [label=0]
	"{'q2', 'q4', 'q0'}" -> "{'q1', 'q3', 'q5'}" [label=1]
	"{'q1', 'q3', 'q5'}" -> "{'q2', 'q4', 'q0'}" [label=0]
	"{'q1', 'q3', 'q5'}" -> "{'q2', 'q4', 'q0'}" [label=1]
}
