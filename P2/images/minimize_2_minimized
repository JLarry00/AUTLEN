	rankdir=LR
	"" [shape=none]
	"{'q4', 'q0', 'q2'}" [shape=doublecircle]
	"{'q5', 'q3', 'q1'}" [shape=circle]
	"" -> "{'q4', 'q0', 'q2'}"
	"{'q4', 'q0', 'q2'}" -> "{'q5', 'q3', 'q1'}" [label=0]
	"{'q4', 'q0', 'q2'}" -> "{'q5', 'q3', 'q1'}" [label=1]
	"{'q5', 'q3', 'q1'}" -> "{'q4', 'q0', 'q2'}" [label=0]
	"{'q5', 'q3', 'q1'}" -> "{'q4', 'q0', 'q2'}" [label=1]
}
