// Automata
digraph {
	rankdir=LR
	"" [shape=none]
	"{'Initial'}" [shape=circle]
	"{'B'}" [shape=doublecircle]
	"{'Empty'}" [shape=circle]
	"" -> "{'Initial'}"
	"{'Initial'}" -> "{'B'}" [label=a]
	"{'Initial'}" -> "{'Empty'}" [label=b]
	"{'B'}" -> "{'B'}" [label=a]
	"{'B'}" -> "{'B'}" [label=b]
	"{'Empty'}" -> "{'Empty'}" [label=a]
	"{'Empty'}" -> "{'Empty'}" [label=b]
}
