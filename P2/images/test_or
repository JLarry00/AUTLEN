// Automata
digraph {
	rankdir=LR
	"" [shape=none]
	q6 [shape=circle]
	q7 [shape=doublecircle]
	q4 [shape=circle]
	q5 [shape=circle]
	q0 [shape=circle]
	q1 [shape=circle]
	q2 [shape=circle]
	q3 [shape=circle]
	"" -> q6
	q0 -> q1 [label=a]
	q2 -> q3 [label=b]
	q4 -> q2 [label="λ"]
	q4 -> q0 [label="λ"]
	q1 -> q5 [label="λ"]
	q3 -> q5 [label="λ"]
	q6 -> q7 [label="λ"]
	q6 -> q4 [label="λ"]
	q5 -> q7 [label="λ"]
	q5 -> q4 [label="λ"]
}
