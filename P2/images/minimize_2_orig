// Automata
digraph {
	rankdir=LR
	"" [shape=none]
	q0 [shape=doublecircle]
	q1 [shape=circle]
	q2 [shape=doublecircle]
	q3 [shape=circle]
	q4 [shape=doublecircle]
	q5 [shape=circle]
	"" -> q0
	q0 -> q1 [label=0]
	q0 -> q1 [label=1]
	q1 -> q2 [label=0]
	q1 -> q2 [label=1]
	q2 -> q3 [label=0]
	q2 -> q3 [label=1]
	q3 -> q4 [label=0]
	q3 -> q4 [label=1]
	q4 -> q5 [label=0]
	q4 -> q5 [label=1]
	q5 -> q0 [label=0]
	q5 -> q0 [label=1]
}
